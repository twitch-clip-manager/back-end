{
  "name": "back-end",
  "version": "1.0.0",
  "description": "twitch-clip-manager",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest -i",
    "lint": "eslint .",
    "dbon": "mkdir -p ./db && mongod --dbpath ./db"
  },
  "jest": {
    "setupFiles": [
      "./__test__/lib/jest-setup.js"
    ],
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "/__test__/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 40,
        "lines": 40,
        "statements": 40
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twitch-clip-manager/back-end.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/twitch-clip-manager/back-end/issues"
  },
  "homepage": "https://github.com/twitch-clip-manager/back-end#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.0",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "jest": "^22.4.2",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.11",
    "moxios": "^0.4.0",
    "superagent": "^3.8.2"
  }
}
