{
  "name": "back-end",
  "version": "1.0.0",
  "description": "twitch-clip-manager",
  "main": "index.js",
  "scripts": {
    "start": "node index.js", 
    "test": "jest -i",
    "lint": "eslint ."
  },
  "jest": {
    "setupFiles": [
      "./__test__/lib/jest-setup.js"
    ],
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "/__test__/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twitch-clip-manager/back-end.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/twitch-clip-manager/back-end/issues"
  },
  "homepage": "https://github.com/twitch-clip-manager/back-end#readme",
  "dependencies": {
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.0",
    "express": "^4.16.3",
    "jest": "^22.4.2",
    "superagent": "^3.8.2"
  }
}
